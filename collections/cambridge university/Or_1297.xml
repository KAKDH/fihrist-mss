                    <msContents><summary>E. Syriac Psalter. </summary>
                        <msItem>
                            <title type="desc">East Syriac liturgical Psalter. </title><textLang mainLang="syc">Syriac</textLang>
                            <note>Included are other texts for the daily offices. The whole contents
                                are: </note>
                        </msItem>
                        <msItem>
                            <locus>ff. 1r-128r</locus>
                            <title type="desc">The Psalms in the Peshitta version. </title>
                        </msItem>
                        <msItem>
                            <locus>ff. 129r-133v</locus>
                            <title type="desc">Biblical canticles. </title>
                        </msItem>
                        <msItem>
                            <locus>ff. 134v-148r</locus>
                            <title type="desc"><hi rend="italic">Teshbḥata</hi> for the
                                ecclesiastical year.</title>
                        </msItem>
                        <msItem>
                        <locus>ff. 140v-147r</locus>
                        <title type="desc"><hi rend="italic">Teshbḥata</hi> for ordinary days.</title>
                            </msItem>
                        <msItem>
                            <locus>ff. 148r-155v</locus>
                            <title type="desc">Litanies.</title>
                        </msItem>
                        <msItem>
                            <locus>ff. 155v-187v</locus>
                            <title type="desc">Martyrs' anthems. </title>
                        </msItem>
                    </msContents>

