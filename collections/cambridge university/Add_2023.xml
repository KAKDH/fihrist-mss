                                <persName key="person_67257819" role="ant" change="#role-assignment">Moses bar Kepha, </persName>
                                <persName key="person_88967224" role="ant" change="#role-assignment">Basil of Caesarea, </persName>
                                <persName key="person_256760175" role="ant" change="#role-assignment">Philoxenus, </persName>
                                <persName key="person_51687951" role="ant" change="#role-assignment">Cyril of Alexandria, </persName>
                                <persName key="person_59878274" role="ant" change="#role-assignment">Dionysius the Areopagite, </persName>
                                <persName key="person_46897094" role="ant" change="#role-assignment #ids-cleanup">Gregory Nazienzen, </persName>
                                <persName key="person_51661489" role="ant" change="#role-assignment">Jacob of Serug, </persName>
